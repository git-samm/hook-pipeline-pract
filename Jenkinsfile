pipeline {
	agent any  
		stages{
			stage('1-make a left'){
			steps{ 
				echo "walk...."
				}
			}
			stage('2-make a right'){
				steps{
					echo "walk..."
				}
			}
			stage('3-make another left'){
				steps{
					echo "walk..."
				}
			}
			stage('4-cross the street'){
				steps{
					echo "walk..."
				}
			}

		} 
		
	
}
